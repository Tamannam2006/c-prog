#include <stdio.h>
int main() {
    int books[100], n, i, key, found = 0;
    printf("Enter number of books: ");
    scanf("%d", &n);
    printf("Enter Book IDs:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &books[i]);
    }
    printf("Enter Book ID to search: ");
    scanf("%d", &key);
    for(i = 0; i < n; i++) {
        if(books[i] == key) {
            found = 1;
            break;
        }
    }
    if(found)
        printf("Book is Available.\n");
    else
        printf("Book is NOT Available.\n");
}
